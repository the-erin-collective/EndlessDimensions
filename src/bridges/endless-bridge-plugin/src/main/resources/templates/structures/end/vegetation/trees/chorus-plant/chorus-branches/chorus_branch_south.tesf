num length = 3+randomInt(2);
bool allow = true;

for (num y = 1; y < length; y = y + 1) {
    if (getBlock(0,y,0) != "minecraft:air") {
        allow = false;
        fail;
    }
}

if (allow == true) {
    block(0,0,0,"DIM_TREE_LOG[up=true,north=true]");
    for (num y = 1; y < length; y = y + 1) {
        block(0,y,0,"DIM_TREE_LOG[up=true,down=true]");
    }
}

block(0,length,0,"DIM_TREE_LEAVES[age=5]");



