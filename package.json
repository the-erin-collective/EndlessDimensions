{
  "name": "endless-dimensions",
  "version": "1.0.0",
  "description": "A Minecraft mod that replicates 20w14infinite snapshot mechanics using Moud",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node build.js",
    "build:ts": "tsc",
    "build:pack": "node build.js",
    "dev": "tsc --watch",
    "clean": "rimraf dist build",
    "lint": "tsc --noEmit",
    "test": "npm run lint",
    "prepare": "npm run build"
  },
  "keywords": ["minecraft", "mod", "moud", "typescript", "20w14infinite"],
  "author": "EndlessDimensions",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "moud": "^1.0.0"
  }
}
